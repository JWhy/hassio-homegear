{
    "name": "Homegear",
    "version": "0.1.7",
    "slug": "homegear",
    "description": "Homegear hassio add-on",
    "auto_uart": true,
    "url": "https://github.com/tringler/hassio-homegear",
    "startup": "services",
    "arch": [
      "aarch64",
      "amd64",
      "armhf",
      "armv7",
      "i386"
    ],
    "boot": "auto",
    "ports": {
      "2001/tcp": 2001
    },
    "ports_description": {
      "2001/tcp": "HomeGear UI"
    },
    "map": [
      "config:rw",
      "ssl"
    ],
    "schema": {
      "ssl": "bool"
    },
    "devices": [
      "/dev/spidev0.0:/dev/spidev0.0:rwm"
    ],
    "gpio": true,
    "options": {
      "ssl": false
  }
}
